<form method="post" action="{{ path('app_category_delete', {'idcategory': category.idcategory}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ category.idcategory) }}">
    <span>    <button class="btn"><i class="fi fi-rr-trash"></i></button>  </span>
</form>
